        "ng-packagr": "^4.1.0",
        "protractor": "~5.4.0",
        "ts-node": "~7.0.1",
        "tsickle": ">=0.25.5",
        "tslib": "^1.7.1",
        "tslint": "~5.11.0",
        "typescript": "~2.7.2",
        "core-js": "^2.5.7",
        "rxjs": "^6.2.2",
        "zone.js": "^0.8.19"
    }
}
