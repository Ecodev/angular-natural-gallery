{
    "name": "@ecodev/angular-natural-gallery",
    "scripts": {
        "ng": "ng",
        "dev": "ng serve --port 4201",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "build-lib": "ng build angular-natural-gallery && cp -v README.md dist/angular-natural-gallery/ && gulp copy:themes",
        "build-docs": "yarn build-lib && ng build demo --prod --aot --base-href /angular-natural-gallery/ && cp dist/demo/index.html dist/demo/404.html",
        "release": "yarn build-lib && cd dist/angular-natural-gallery/ && yarn publish --access public"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Ecodev/angular-natural-gallery"
    },
    "author": {
        "name": "Samuel Baptista",
        "email": "samuel.baptista@ecodev.ch"
    },
    "keywords": [
        "gallery",
        "image",
        "images",
        "slideshow",
        "lightbox",
        "zoom",
        "list",
        "autoload",
        "lazyload",
        "infinite",
        "scroll",
        "infinite scroll"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Ecodev/angular-natural-gallery/issues"
    },
    "dependencies": {
        "@ecodev/natural-gallery-js": "^4.0.0"
    },
    "peerDependencies": {
        "@angular/common": "^6.1.3"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.7.0",
        "@angular-devkit/build-ng-packagr": "^0.7.4",
        "@angular/cli": "6.1.4",
        "@angular/compiler-cli": "^6.1.3",
        "@angular/language-service": "^6.1.3",
        "@angular/platform-browser": "^6.1.3",
        "@angular/platform-browser-dynamic": "^6.1.3",
        "@angular/animations": "^6.1.3",
        "@angular/compiler": "^6.1.3",
        "@angular/core": "^6.1.3",
        "@angular/router": "^6.1.3",
        "@types/jasmine": "~2.8.8",
        "@types/jasminewd2": "~2.0.2",
        "@types/node": "~10.7.1",
        "codelyzer": "^4.4.4",
        "gulp": "^3.9.1",
        "gulp-flatten": "^0.4.0",
        "gulp-shell": "^0.6.5",
        "jasmine-core": "~3.2.1",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~3.0.0",
        "karma-chrome-launcher": "~2.2.0",
