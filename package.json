{
    "name": "@ecodev/angular-natural-gallery",
    "scripts": {
        "ng": "ng",
        "dev": "ng serve --port 4205",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "build-lib": "ng build angular-natural-gallery && cp -v README.md dist/angular-natural-gallery/ && scss-bundle -e ./projects/angular-natural-gallery/src/lib/_natural-gallery.theme.scss -d ./dist/angular-natural-gallery/theming/_natural-gallery.theme.scss && cd dist/angular-natural-gallery && yarn version --no-git-tag-version --new-version `git describe --tags --always`",
        "build-docs": "yarn build-lib && ng build demo --prod --aot --base-href /angular-natural-gallery/ && cp dist/demo/index.html dist/demo/404.html",
        "release": "yarn build-lib && cd dist/angular-natural-gallery/ && yarn publish --access public",
        "llink": "cd dist/angular-natural-gallery && (yarn unlink ; yarn link)"

    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Ecodev/angular-natural-gallery"
    },
    "author": {
        "name": "Samuel Baptista",
        "email": "samuel.baptista@ecodev.ch"
    },
    "keywords": [
        "gallery",
        "image",
        "images",
        "slideshow",
        "lightbox",
        "zoom",
        "list",
        "autoload",
        "lazyload",
        "infinite",
        "scroll",
        "infinite scroll"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Ecodev/angular-natural-gallery/issues"
    },
    "dependencies": {
        "photoswipe": "^4.1.3"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.802.0",
        "@angular-devkit/build-ng-packagr": "~0.802.0",
        "@angular/animations": "^8.2.7",
        "@angular/cli": "~8.2.0",
        "@angular/common": "~8.2.7",
        "@angular/compiler": "^8.2.7",
        "@angular/compiler-cli": "^8.2.7",
        "@angular/core": "^8.2.7",
        "@angular/language-service": "~8.2.0",
        "@angular/platform-browser": "~8.2.0",
        "@angular/platform-browser-dynamic": "~8.2.0",
        "@angular/router": "~8.2.0",
        "@ecodev/natural-gallery-js": "^5.0.0",
        "@types/jasmine": "~3.3.8",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "~12.6.9",
        "codelyzer": "^5.0.0",
        "jasmine-core": "~3.4.0",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~4.2.0",
        "karma-chrome-launcher": "~3.0.0",
        "karma-coverage-istanbul-reporter": "~2.1.0",
        "karma-jasmine": "~2.0.1",
        "karma-jasmine-html-reporter": "^1.4.0",
        "ng-packagr": "^5.3.0",
        "protractor": "~5.4.0",
        "puppeteer": "^1.19.0",
        "rxjs": "^6.5.3",
        "scss-bundle": "^2.5.1",
        "ts-node": "~8.3.0",
        "tsickle": "^0.36.0",
        "tslib": "^1.9.0",
        "tslint": "~5.18.0",
        "typescript": "~3.5.3",
        "zone.js": "~0.9.1"
    }
}
